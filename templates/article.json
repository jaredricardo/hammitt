    "image_with_text_zNFG4e": {
      "type": "image-with-text",
      "blocks": {
        "text_PLAEGi": {
          "type": "text",
          "settings": {
            "text": "<p>\tLast but not least, we have the Charles Crossbody Large in Pewter Pebble, crafted with a neutral grey brown soft pebbled leather. This neutral bag elevates simple summer outfits, giving them a polished feel, yet it remains a versatile option for whatever adventure your summer getaway calls for.<\/p>",
            "text_style": "body"
          }
        }
      },
