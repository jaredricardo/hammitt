    "image_with_text_z8gikk": {
      "type": "image-with-text",
      "blocks": {
        "text_Dw4APk": {
          "type": "text",
          "settings": {
            "text": "<p>\tIf you prefer a lighter-colored yet still versatile colorway for your travels, the Charles in Courtyard Cream is for you. The subtle contrast of the cream pebbled leather with brown leather trim accents creates a unique aesthetic that makes a statement and is effortlessly elegant. Whether you are strolling along the beach or dressing up for dinner, this colorway is a versatile option for any occasion, and may become your new go-to bag.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
