    "image_with_text_RbpJLJ": {
      "type": "image-with-text",
      "blocks": {
        "text_9yPQHy": {
          "type": "text",
          "settings": {
            "text": "<p>If you prefer to travel light, the Charles Med in Mahogany Pebble\/Brushed Gold Red Zip may be more your style. The warm, medium brown hue and yellow undertones of the mahogany pebbled leather pair beautifully with bright summer colors or neutrals, so you won’t have to worry about fitting all of your must-have handbags into your carry-on. The Charles has you covered! <\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "text_9yPQHy"
      ],
