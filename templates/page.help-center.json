        "7054ddf5-bdc9-4847-89c0-135f97bd5dad": {
          "type": "faq",
          "settings": {
            "title": "How do I contact Hammitt?",
            "answer": "<p>Email <a href=\"mailto:wecare@hammitt.com\"><span style=\"text-decoration:underline\">wecare@hammitt.com</span></a>, or call <br/>10:30am to 5pm PST Monday & 8am to 5pm PST Tuesday - Friday <strong>(833) 426-6488</strong> and a Concierge expert will help you personally - or direct your inquiry to the right team member!<strong> </strong></p>",
            "category": "Other",
            "image_location": "below_answer"
